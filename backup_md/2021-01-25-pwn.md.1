  <span class="pl-c1">setresgid</span>(gid, gid, gid);
  
  <span class="pl-c1">printf</span>(<span class="pl-s"><span class="pl-pds">"</span>Would you like to read the flag? (yes/no)<span class="pl-cce">\n</span><span class="pl-pds">"</span></span>);

